<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" text="Label" wrapText="true">
            <HBox.margin>
                <Insets right="8.0" />
            </HBox.margin>
         <font>
            <Font name="Arial" size="13.0" />
         </font>
        </Label>
        <ImageView fx:id="displayPicture" fitHeight="72.0" fitWidth="72.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets right="8.0" />
            </HBox.margin>
        </ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
