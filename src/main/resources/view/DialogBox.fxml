<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.text.Font?>
<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" minHeight="-Infinity" styleClass="genie-dialog" text="Label" wrapText="true">
            <font>
                <Font name="Helvetica" size="13.0"/>
            </font>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
            <HBox.margin>
                <Insets left="10.0" right="10.0" />
            </HBox.margin>
            <padding>
                <Insets bottom="16.0" left="21.0" right="21.0" top="16.0" />
            </padding>
        </Label>
        <ImageView fx:id="displayPicture" fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets left="10.0" right="10.0" />
            </HBox.margin>
        </ImageView>
    </children>
    <padding>
        <Insets bottom="14.0" left="5.0" right="5.0" top="14.0" />
    </padding>
</fx:root>
