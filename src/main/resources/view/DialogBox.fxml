<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Polygon?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="Infinity" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" alignment="TOP_LEFT" contentDisplay="TOP" style="-fx-background-color: #6b99d6; -fx-padding: 8; -fx-border-radius: 5px; -fx-background-radius: 5px; -fx-font-size: 14;" text="Label" textFill="WHITE" wrapText="true" >
         <font>
            <Font name="Malgun Gothic Semilight" size="12.0" />
         </font>
      </Label>
      <Polygon fill="#6a98d4" points="21.0, 55.25, 36.0, 67.0, 21.0, 67.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0">
         <HBox.margin>
            <Insets top="8.0" />
         </HBox.margin>
      </Polygon>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets left="4.0" right="4.0" />
         </HBox.margin></ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</fx:root>
