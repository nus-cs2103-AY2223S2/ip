<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<fx:root fx:id="hbox" alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0"
         style="-fx-background-color: azure; -fx-border-color: lightgrey transparent lightgrey transparent;" type="javafx.scene.layout.HBox"
         xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" cache="true" maxHeight="1.7976931348623157E308" minHeight="-Infinity" text="Label" wrapText="true">
         <HBox.margin>
            <Insets left="10.0" right="10.0" />
         </HBox.margin>
         <font>
            <Font name="AppleGothic Regular" size="13.0" />
         </font></Label>
      <Circle fx:id="circle" fill="DODGERBLUE" radius="49.0" stroke="BLACK" strokeType="INSIDE">
         <effect>
            <DropShadow />
         </effect></Circle>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
