<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import java.net.URL?>

<fx:root alignment="TOP_RIGHT" maxHeight="Infinity" maxWidth="Infinity" HBox.hgrow="ALWAYS"
    type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <Label fx:id="dialog" text="Label" wrapText="true" maxHeight="Infinity" styleClass="textbox" prefWidth="300"/>
    <ImageView fx:id="displayPicture" fitHeight="70.0" fitWidth="70.0" pickOnBounds="true" preserveRatio="true" />
  </children>
  <padding>
    <Insets bottom="15.0" left="5.0" right="5.0" top="15.0"/>
  </padding>
  <stylesheets>
    <URL value="@../stylesheet/dialogbox.css" />
  </stylesheets>
</fx:root>