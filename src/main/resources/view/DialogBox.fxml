<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox minHeight="15.0" minWidth="15.0" maxHeight="15.0" maxWidth="15.0" prefHeight="15.0" prefWidth="15.0"/>
        <Label fx:id="dialog" minHeight="-Infinity" minWidth="50" text="Label" wrapText="true">
            <font>
                <Font name="Roboto" size="12.0"/>
            </font>
        </Label>
        <HBox minHeight="15.0" minWidth="15.0" maxHeight="15.0" maxWidth="15.0" prefHeight="15.0" prefWidth="15.0"/>
        <ImageView fx:id="displayPicture" fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
        </ImageView>
        <HBox minHeight="15.0" minWidth="15.0" maxHeight="15.0" maxWidth="15.0" prefHeight="15.0" prefWidth="15.0"/>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0"/>
    </padding>
    <stylesheets>
        <URL value="@../css/style.css"/>
    </stylesheets>
</fx:root>
