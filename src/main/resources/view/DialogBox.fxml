               style="-fx-background-color: F34652; -fx-background-radius: 30;"
               text="Label" wrapText="true" HBox.hgrow="ALWAYS">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <padding>
                <Insets bottom="10.0" left="20.0" right="20.0" top="10.0"/>
            </padding>
        </Label>
        <Pane prefHeight="100.0" prefWidth="100.0">
            <children>
                <ImageView fx:id="displayPicture" fitHeight="80.0" fitWidth="80.0" pickOnBounds="true"
                           preserveRatio="true" layoutX="10.0" layoutY="15.0" nodeOrientation="INHERIT"/>
            </children>
        </Pane>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <!--@@author-->
</fx:root>