<fx:root alignment="CENTER_RIGHT" maxHeight="Infinity" maxWidth="1.7976931348623157E308" prefWidth="588.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
