<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
<children>
    <Label fx:id="dialog" alignment="CENTER" minHeight="-Infinity" style="-fx-border-color: blue; -fx-border-radius: 10;" text="Label" wrapText="true" HBox.hgrow="ALWAYS">
        <font>
            <Font name="Comic Sans MS" size="13.0" />
        </font>
        <HBox.margin>
            <Insets />
        </HBox.margin>
        <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
        </padding></Label>
    <ImageView fx:id="displayPicture" fitHeight="72.0" fitWidth="72.0" pickOnBounds="true" preserveRatio="true">
        <HBox.margin>
            <Insets left="10.0" right="10.0" />
        </HBox.margin></ImageView>
</children>
<padding>
    <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
</padding>
</fx:root>
