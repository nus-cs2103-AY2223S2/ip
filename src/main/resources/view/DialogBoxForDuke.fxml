<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" lineSpacing="1.0" style="-fx-background-color: white; -fx-background-radius: 50;" text="Label" textAlignment="JUSTIFY" textFill="BLACK" wrapText="true">
            <HBox.margin>
                <Insets />
            </HBox.margin>
            <font>
                <Font name="Franklin Gothic Book" size="12.0" />
            </font>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding></Label>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="97.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets bottom="20.0" left="10.0" right="10.0" top="10.0" />
            </HBox.margin></ImageView>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
